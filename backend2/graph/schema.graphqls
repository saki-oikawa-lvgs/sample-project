# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type Post {
  id: ID!
  text: String!
  done: Boolean!
  user: User!
}

type User {
  id: ID!
  name: String!
}

type Query {
  posts: [Post!]!
}

input NewPost {
  text: String!
  userId: String!
}

type Mutation {
  createPost(input: NewPost!): Post!
}
